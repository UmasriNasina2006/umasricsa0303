#include <stdio.h>
#define INF 9999
#define MAX 10

void prim(int graph[MAX][MAX], int n) {
    int selected[MAX] = {0};
    int edges = 0;
    selected[0] = 1; // Start from vertex 0

    printf("Edge\tWeight\n");
    while (edges < n - 1) {
        int min = INF, x = 0, y = 0;
        for (int i = 0; i < n; i++) {
            if (selected[i]) {
                for (int j = 0; j < n; j++) {
                    if (!selected[j] && graph[i][j] && graph[i][j] < min) {
                        min = graph[i][j];
                        x = i; y = j;
                    }
                }
            }
        }
        printf("%d - %d\t%d\n", x, y, min);
        selected[y] = 1;
        edges++;
    }
}

int main() {
    int n, graph[MAX][MAX];
    printf("Enter number of vertices: ");
    scanf("%d", &n);
    printf("Enter adjacency matrix (0 for no edge):\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &graph[i][j]);
    prim(graph, n);
    return 0;
}
