#include <stdio.h>
int fib(int n, int f[])
{
    if (n == 0) return f[0] = 0;
    if (n == 1) return f[1] = 1;
    if (f[n] != -1) return f[n];
    return f[n] = fib(n - 1, f) + fib(n - 2, f);
int fib(int n)
{
    if (n == 0) return 0;
    if (n == 1) return 1;
    return fib(n - 1) + fib(n - 2);
}
int main()
{
    int n, i;
    printf("Enter number of terms: ");
    scanf("%d", &n);
    int f[100];
    for (i = 0; i < n; i++) f[i] = -1;
    printf("Fibonacci Series: ");
    for (i = 0; i < n; i++)
        printf("%d ", fib(i, f));
        printf("%d ", fib(i));

    return 0;
}
