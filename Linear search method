#include <stdio.h>

int main() {
    int arr[100], n, key, i, found = 0;

    printf("Enter size: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for (i = 0; i < n; i++) scanf("%d", &arr[i]);

    printf("Enter number to search: ");
    scanf("%d", &key);

    for (i = 0; i < n; i++) {
        if (arr[i] == key) {
            printf("Found at position %d\n", i);
            found = 1; break;
        }
    }
    if (!found) printf("Not found\n");
    return 0;
}
